    package="com.example.homesweethome">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".LoginPage">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".HomePage"
            android:theme="@style/AppTheme.NoActionBar"
            />
        <activity android:name=".SingleArtifactPage" />
        <activity android:name=".AddPage" />
        <activity android:name=".SingleImagePage" />
        <activity android:name=".register.EmailValidationActivity" />
        <activity android:name=".register.RegisterActivity" />
        <activity android:name=".register.RegisterSuccessActivity" />
        <activity android:name=".register.RetrievePasswordActivity" />

    </application>

</manifest>